
#include <iostream>
#include <string>
using namespace std;


template<class Typename ,class Typeage>           
class person
{
public:
    Typename = m_name;
    Typeage =  m_age;
    person(Typename name, Typeage  age)
    {
        this->m_age = age;
        this->m_name = name;
    }

};





int main()
{
    person<string, int> p("张三",18);  //这里得要声明类型
    return 0;
    system("pause");
}

//和函数模板的区别：
//1.不能自动推到类型（但好像现在可以自动推到了）
//2.
